{
    "name": "willy68/pgframework-mono",
    "description": "Application skelton for pgframework",
    "authors": [
        {
            "name": "William Lety",
            "email": "william.lety@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "package/pgframework"
        },
        {
            "type": "git",
            "url": "package/pgconsole"
        }
    ],
    "require": {
        "http-interop/response-sender": "^1.0",
        "willy68/pgframework": "dev-master",
        "ext-json": "*",
        "ext-pdo": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Application\\Console\\": "bin/src"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "require-dev": {
        "middlewares/whoops": "^2.0",
        "squizlabs/php_codesniffer": "^3.5",
        "phpunit/phpunit": "^8.5",
        "robmorgan/phinx": "^0.11.4",
        "fzaninotto/faker": "^1.9",
        "odan/phinx-migrations-generator": "^4.6",
        "symfony/console": "^5.0",
        "willy68/pgconsole": "dev-master"
    },
    "scripts": {
        "pre-update-cmd": [
            "@php ComposerEvent.php"
        ]
    }
}
